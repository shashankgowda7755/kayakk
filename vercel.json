{
  "version": 2,
  "name": "munroe-island-kayaking",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ],
  "redirects": [
    {
      "source": "/images/(.*)",
      "destination": "/client/public/images/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}