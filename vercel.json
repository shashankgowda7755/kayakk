{
  "version": 2,
  "name": "munroe-island-kayaking",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    }
  ],
  "redirects": [
    {
      "source": "/images/(.*)",
      "destination": "/client/public/images/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server/index.ts": {
      "maxDuration": 30
    }
  }
}